&pio {
        sia81xx_gpio_L: sia81xx_gpio_L {
                pins_cmd_dat {
                        pinmux = <PINMUX_GPIO5__FUNC_GPIO5>;
                        slew-rate = <1>;
                        bias-disable;
                };
        };

        sia81xx_gpio_R: sia81xx_gpio_R {
                pins_cmd_dat {
                        pinmux = <PINMUX_GPIO170__FUNC_GPIO170>;
                        slew-rate = <1>;
                        bias-disable;
                };
        };
};

&odm {
       sia81xx_L: sia81xx@L{
                compatible = "si,sia81xx";
                si,si_pa_type = "sia815T";
                si,si_pa_reset = <&pio 5 0>;
                si,si_pa_owi = <&pio 5 0>;
                pinctrl-names = "si_pa_gpio";
                pinctrl-0 = <&sia81xx_gpio_L>;
                si,si_pa_disable_pin = <0>;
                channel_num = <0>;
                owi_mode = <1>;
        };
        sia81xx_R: sia81xx@R{
                compatible = "si,sia81xx";
                si,si_pa_type = "sia815T";
                si,si_pa_reset = <&pio 170 0>;
                si,si_pa_owi = <&pio 170 0>;
                pinctrl-names = "si_pa_gpio";
                pinctrl-0 = <&sia81xx_gpio_R>;
                si,si_pa_disable_pin = <0>;
                channel_num = <1>;
                owi_mode = <1>;
        };
};

&i2c7 {
        #address-cells = <1>;
        #size-cells = <0>;
        clock-frequency = <400000>;

        sia81xx_i2c_L: sia81xx_i2c@L {
                compatible = "si,sia81xx-i2c";
                reg = <0x29>;
                si,sipa-dev = <&sia81xx_L>;
        };
        sia81xx_i2c_R: sia81xx_i2c@R {
                compatible = "si,sia81xx-i2c";
                reg = <0x28>;
                si,sipa-dev = <&sia81xx_R>;
        };

        aw87xxx_pa_5B@5B {
                compatible = "awinic,aw87xxx_pa_2_x_0";
                reg = <0x5B>;
                reset-gpio = <&pio 5 0>;
                dev_index = < 0 >;
                status = "okay";
        };
        aw87xxx_pa_58@58 {
                compatible = "awinic,aw87xxx_pa_2_x_0";
                reg = <0x58>;
                reset-gpio = <&pio 170 0>;
                dev_index = < 1 >;
                status = "okay";
        };

	dio4485: dio4485@42 {
		compatible = "oplus,typec-switch-i2c";
		reg = <0x42>;
		// add for supporting type-c headphone detect bypass
		oplus,hp-bypass = <1>;
		status = "okay";
	};
};

&sound {
        si,sia81xx-max-num = <2>;
        si,sia81xx-aux-devs = <&sia81xx_L>, <&sia81xx_R>;
        si,sia81xx-aux-devs-prefix = "SpkrLeft", "SpkrRight";
        /* 2024/5/28, add for dual PA special dapm handle to resolve pop sound when PA closing */
        oplus,special-pa-dapm = <1>;
};

&accdet{
        // eint_use_ext_res = <1>;
        // headset-eint-trig-mode = <2>;
        headset-three-key-threshold = <0 80 170 306>;
        hp-eint0-disable = <1>;
        headset-eint0-disable = <1>;
        hp-plug-in-check = <1>;
};
&speech_usip_mem{
	adsp-phone-call-enh-enable = <0x5>;
};
